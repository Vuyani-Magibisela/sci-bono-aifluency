{
    "name": "sci-bono/ai-fluency-lms",
    "description": "Sci-Bono AI Fluency Learning Management System API",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "firebase/php-jwt": "^6.10",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Controllers\\": "controllers/",
            "App\\Models\\": "models/",
            "App\\Middleware\\": "middleware/",
            "App\\Utils\\": "utils/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
